{
  "extends": ["prettier"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "default-param-last": "off",
    "padding-line-between-statements": "off",
    "no-eq-null": "off",
    "eqeqeq": [
      "error",
      "always",
      {
        "null": "ignore"
      }
    ],
    "prettier/prettier": "error"
  },
  "plugins": ["prettier"]
}
